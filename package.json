    "@types/shortid": "^0.0.29",
    "@types/styled-components": "^4.4.1",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
