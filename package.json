    "react-dev-utils": "^10.0.0",
    "react-test-renderer": "^16.12.0",
    "resolve": "1.14.1",
    "style-loader": "1.1.2",
    "terser-webpack-plugin": "2.3.1",
    "ts-pnp": "1.1.5",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
